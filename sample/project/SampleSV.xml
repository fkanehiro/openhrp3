            <property name="setupDirectory" value="$(PROJECT_DIR)/../controller/SampleSV"/>
            <property name="setupCommand" value="SampleSV$(BIN_SFX)"/>
            <property name="BODY.translation" value="0.0 -1.0 0.1 "/>
